{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ean": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "blanco",
        "negro",
        "azul",
        "verde",
        "rojo",
        "marron",
        "celeste",
        "rosa"
      ]
    },
    "model": {
      "type": "string"
    },
    "sku": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "features": {
      "type": "blocks"
    },
    "brand": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::brand.brand"
    },
    "principal_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::categoria.categoria"
    },
    "secondary_category": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::categoria.categoria"
    },
    "brand_line": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::products-line.products-line",
      "inversedBy": "products"
    }

  }
}
